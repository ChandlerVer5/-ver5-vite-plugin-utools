{
  "name": "@ver5/vite-plugin-utools",
  "version": "0.2.7",
  "description": "Utools for Vite by ChandlerVer5",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./utools": {
      "types": "./utools.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "utools.d.ts",
    "templates"
  ],
  "scripts": {
    "build": "tsup-node && npm run build:scripts",
    "build:scripts": "tsup-node --no-config scripts/template.ts --format cjs --clean false",
    "dev": "npm run build -- --watch & npm run build:scripts -- --watch",
    "release": "npm run build && bumpp --commit --tag --push && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chandlerver5/vite-plugin-utools.git"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "utools"
  ],
  "author": "chandlerver5",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chandlerver5/vite-plugin-utools/issues"
  },
  "homepage": "https://github.com/chandlerver5/vite-plugin-utools#readme",
  "devDependencies": {
    "@types/babel__core": "^7.1.19",
    "@types/babel__generator": "^7.6.4",
    "@types/node": "^16.11.65",
    "@types/resolve": "^1.20.2",
    "bumpp": "^7.2.0",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@babel/generator": "^7.19.5",
    "@babel/parser": "^7.19.4",
    "@electron/asar": "^3.2.3",
    "@jsbits/escape-regex-str": "^1.0.3",
    "@rollup/pluginutils": "^4.2.1",
    "magic-string": "^0.30.0",
    "original-fs": "^1.2.0",
    "picocolors": "^1.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
